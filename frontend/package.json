    "lint-staged": "^16.2.7",
    "postcss": "8.5.6",
    "prettier": "^3.8.1",
    "tailwindcss": "4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.18"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
